<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bitc.java404.mapper.CatCateMapper">

	<select id="selectCateList" resultType="com.bitc.java404.dto.CatCateDto">
		<![CDATA[ 
			SELECT
				cate_code, cate_name
			FROM
				t_category
			ORDER BY
				cate_code DESC
		]]>
	</select>
	
</mapper>